package View;

import java.util.ArrayList;
import java.util.Iterator;

import javax.swing.*;

public class MapFrame extends JFrame {
	
	JPanel panel;
	
	public MapFrame() {
		panel = new JPanel();
		panel.setLayout(null);
		panel.setBounds(0, 0, 300, 300);
		getContentPane().add(panel);
		setSize(300, 300);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
	}
	
